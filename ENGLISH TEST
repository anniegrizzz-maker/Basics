score = 0

test = input('–•–æ—Ç–∏—Ç–µ –ø—Ä–æ–π—Ç–∏ —Ç–µ—Å—Ç –Ω–∞ –∑–Ω–∞–Ω–∏–µ –∞–Ω–≥–ª–∏–π—Å–∫–æ–≥–æ? ')
if test.lower() == '–¥–∞':
    print('–•–æ—Ä–æ—à–æ! –î–∞–≤–∞–π—Ç–µ –Ω–∞—á–Ω–µ–º!\n')


    answer1 = input('–ö–∞–∫ –±—É–¥–µ—Ç –Ω–∞ –∞–Ω–≥–ª–∏–π—Å–∫–æ–º –ö–û–°–ú–û–°? ')
    if answer1.lower() == 'space':
        print('‚úÖ –ü—Ä–∞–≤–∏–ª—å–Ω–æ!\n')
        score += 10
    else:
        print('‚ùå –ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ!\n')

    answer2 = input('–ö–∞–∫ –±—É–¥–µ—Ç –Ω–∞ –∞–Ω–≥–ª–∏–π—Å–∫–æ–º –ö–û–®–ö–ê? ')
    if answer2.lower() == 'cat':
        print('‚úÖ –ü—Ä–∞–≤–∏–ª—å–Ω–æ!\n')
        score += 10
    else:
        print('‚ùå –ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ!\n')


    answer3 = input('–ö–∞–∫ –±—É–¥–µ—Ç –Ω–∞ –∞–Ω–≥–ª–∏–π—Å–∫–æ–º –°–û–õ–ù–¶–ï? ')
    if answer3.lower() == 'sun':
        print('‚úÖ –ü—Ä–∞–≤–∏–ª—å–Ω–æ!\n')
        score += 10
    else:
        print('‚ùå –ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ!\n')


    answer4 = input('–ö–∞–∫ –±—É–¥–µ—Ç –Ω–∞ –∞–Ω–≥–ª–∏–π—Å–∫–æ–º –í–û–î–ê? ')
    if answer4.lower() == 'water':
        print('‚úÖ –ü—Ä–∞–≤–∏–ª—å–Ω–æ!\n')
        score += 10
    else:
        print('‚ùå –ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ!\n')

    answer5 = input('–ö–∞–∫ –±—É–¥–µ—Ç –Ω–∞ –∞–Ω–≥–ª–∏–π—Å–∫–æ–º –õ–û–î–ö–ê? ')
    if answer5.lower() == 'boat':
        print('‚úÖ –ü—Ä–∞–≤–∏–ª—å–Ω–æ!\n')
        score += 10
    else:
        print('‚ùå –ù–µ–ø—Ä–∞–≤–∏–ª—å–Ω–æ!\n')

    print(f'üéì –í–∞—à –∏—Ç–æ–≥–æ–≤—ã–π –±–∞–ª–ª: {score} –∏–∑ 50')


    if score == 50:
        level = 'Advanced üåü'
    elif score >= 30:
        level = 'Intermediate üëç'
    elif score >= 10:
        level = 'Beginner üßê'
    else:
        level = '–ù–∞—á–∏–Ω–∞–π —Å –æ—Å–Ω–æ–≤ üòÖ'

    print(f'–í–∞—à —É—Ä–æ–≤–µ–Ω—å: {level}')

elif test.lower() == '–Ω–µ—Ç':
    print('–ü—Ä–∏—Ö–æ–¥–∏—Ç–µ –≤ —Å–ª–µ–¥—É—é—â–∏–π —Ä–∞–∑!')
